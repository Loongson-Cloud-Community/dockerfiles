REGISTRY?=cr.loongnix.cn
ORGANIZATION?=ceph
REPOSITORY?=ceph
TAG?=16

IMAGE=$(REGISTRY)/$(ORGANIZATION)/$(REPOSITORY):$(TAG)

DAEMON_REPOSITORY=daemon
DAEMON_IMAGE=$(REGISTRY)/$(ORGANIZATION)/$(DAEMON_REPOSITORY):$(TAG)

default: image

image:
	make -C ./daemon-base
	make -C ./daemon
	docker tag $(DAEMON_IMAGE) $(IMAGE)
	
push:
	docker push $(IMAGE)
