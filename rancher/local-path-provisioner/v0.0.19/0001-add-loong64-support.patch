diff --git a/build.sh b/build.sh
new file mode 100755
index 0000000..092e1f4
--- /dev/null
+++ b/build.sh
@@ -0,0 +1,13 @@
+#!/bin/bash
+set -ex
+
+## 构建
+mkdir -p bin
+OTHER_LINKFLAGS="-extldflags -static -s -w"
+LINKFLAGS="-X main.VERSION=v0.0.19"
+CGO_ENABLED=0 go build -ldflags "$LINKFLAGS $OTHER_LINKFLAGS" -o bin/local-path-provisioner
+
+## package docker
+
+docker build -f package/Dockerfile -t cr.loongnix.cn/rancher/local-path-provisioner:v0.0.19 .
+
