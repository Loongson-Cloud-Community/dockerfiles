ORGANIZATION?=library
REPOSITORY?=maven
VERSION?=3.9.6

IMAGE=$(REGISTRY)/$(ORGANIZATION)/$(REPOSITORY):$(VERSION)
FULL_IMAGE=$(IMAGE)-openjdk-21-slim-buster

default: image

image:
	docker build \
		--build-arg http_proxy=$(http_proxy) \
		--build-arg http_proxy=$(http_proxy) \
		-t $(IMAGE) \
		-t $(FULL_IMAGE) \
		.

push:
	docker push $(IMAGE)
	docker push $(FULL_IMAGE)
