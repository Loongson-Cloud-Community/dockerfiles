REGISTRY?=cr.loongnix.cn
ORGANIZATION?=library
REPOSITORY?=rust
TAG?=1.86.0


IMAGE=$(REGISTRY)/$(ORGANIZATION)/$(REPOSITORY)

default: image

image:
	docker build \
		--build-arg http_proxy=$(http_proxy) \
		--build-arg https_proxy=$(https_proxy) \
		-t $(IMAGE):$(TAG) \
		.

push:
	docker push $(IMAGE):$(TAG)
