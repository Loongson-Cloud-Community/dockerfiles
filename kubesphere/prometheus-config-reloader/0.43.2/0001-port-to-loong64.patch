 create mode 100644 cmd/prometheus-config-reloader/Dockerfile.loong64

diff --git a/Makefile b/Makefile
index 1a12138..95b6269 100644
--- a/Makefile
+++ b/Makefile
@@ -2,6 +2,7 @@ SHELL=/bin/bash -o pipefail
 
 GOOS?=$(shell go env GOOS)
 GOARCH?=$(shell go env GOARCH)
+GOPROXY:=https://goproxy.cn
 ifeq ($(GOARCH),arm)
 	ARCH=armv7
 else
@@ -10,7 +11,12 @@ endif
 
 GO_PKG=github.com/prometheus-operator/prometheus-operator
 REPO?=quay.io/prometheus-operator/prometheus-operator
-REPO_PROMETHEUS_CONFIG_RELOADER?=quay.io/prometheus-operator/prometheus-config-reloader
+ifeq ($(GOARCH),loong64)
+	REPO_PROMETHEUS_CONFIG_RELOADER?=cr.loongnix.cn/kubesphere/prometheus-config-reloader
+	TAG?=0.43.2
+else
+	REPO_PROMETHEUS_CONFIG_RELOADER?=quay.io/prometheus-operator/prometheus-config-reloader
+endif
 REPO_PROMETHEUS_OPERATOR_LINT?=quay.io/prometheus-operator/prometheus-operator-lint
 TAG?=$(shell git rev-parse --short HEAD)
 VERSION?=$(shell cat VERSION | tr -d " \t\n\r")
@@ -58,7 +64,7 @@ GO_BUILD_VERSION_LDFLAGS=\
 	-X $(PROMETHEUS_COMMON_PKG)/version.Branch=$(GITHUB_REF:refs/heads/%=%) \
 	-X $(PROMETHEUS_COMMON_PKG)/version.Version=$(VERSION)
 
-GO_BUILD_RECIPE=GOOS=$(GOOS) GOARCH=$(GOARCH) CGO_ENABLED=0 go build -ldflags="-s $(GO_BUILD_VERSION_LDFLAGS)"
+GO_BUILD_RECIPE=GOPROXY=$(GOPROXY) GOOS=$(GOOS) GOARCH=$(GOARCH) CGO_ENABLED=0 go build -ldflags="-s $(GO_BUILD_VERSION_LDFLAGS)"
 
 pkgs = $(shell go list ./... | grep -v /test/ | grep -v /contrib/)
 pkgs += $(shell go list $(GO_PKG)/pkg/apis/monitoring...)
@@ -140,11 +146,15 @@ image: .hack-operator-image .hack-prometheus-config-reloader-image
 	docker build --build-arg ARCH=$(ARCH) --build-arg OS=$(GOOS) -t $(REPO):$(TAG) .
 	touch $@
 
-.hack-prometheus-config-reloader-image: cmd/prometheus-config-reloader/Dockerfile prometheus-config-reloader
+.hack-prometheus-config-reloader-image: cmd/prometheus-config-reloader/Dockerfile prometheus-config-reloader cmd/prometheus-config-reloader/Dockerfile.loong64
 # Create empty target file, for the sole purpose of recording when this target
 # was last executed via the last-modification timestamp on the file. See
 # https://www.gnu.org/software/make/manual/make.html#Empty-Targets
+ifeq ($(GOARCH),loong64)
+	docker build --build-arg ARCH=$(ARCH) --build-arg OS=$(GOOS) -t $(REPO_PROMETHEUS_CONFIG_RELOADER):$(TAG) -f cmd/prometheus-config-reloader/Dockerfile.loong64 .
+else
 	docker build --build-arg ARCH=$(ARCH) --build-arg OS=$(GOOS) -t $(REPO_PROMETHEUS_CONFIG_RELOADER):$(TAG) -f cmd/prometheus-config-reloader/Dockerfile .
+endif
 	touch $@
 
 
diff --git a/cmd/prometheus-config-reloader/Dockerfile.loong64 b/cmd/prometheus-config-reloader/Dockerfile.loong64
new file mode 100644
index 0000000..733318f
--- /dev/null
+++ b/cmd/prometheus-config-reloader/Dockerfile.loong64
@@ -0,0 +1,9 @@
+ARG ARCH="loong64"
+ARG OS="linux"
+FROM cr.loongnix.cn/prometheus/busybox:glibc
+
+ADD prometheus-config-reloader /bin/prometheus-config-reloader
+
+USER nobody
+
+ENTRYPOINT ["/bin/prometheus-config-reloader"]
